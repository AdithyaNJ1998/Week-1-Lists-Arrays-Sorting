class Solution:
def twoSum(self, numbers, target):
left = 0
right = len(numbers) - 1
while left < right:
s = numbers[left] + numbers[right]
if s == target:
return [left + 1, right + 1]  # 1-indexed
if s < target:
print(sol.twoSum([2, 3, 4], 6))        # [1, 3]
print(sol.twoSum([-1, 0], -1))         # [1, 2]
